				//console.log(this); //var ordId = jQuery(this).data('id').toString();
				var ordId = jQuery(this).data('id');
				jQuery.ajax({
  					method: "POST",
  				        url: "<?php echo $this->getUrl('sales/order/contactshop'); ?>id/"+ordId,
				        cache: false,
				       	dataType: "html",
					success: function(result,success) { 
					    console.log('success'); //console.log(result); 
					    jQuery('#search #myOrderModal1').append(result); 
					}
				}).done(function( msg ) { 
    					//jQuery('#myOrderModalall').modal({ show: true });
  				});
			});
			</script>
